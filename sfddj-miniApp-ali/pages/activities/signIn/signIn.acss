/* 箭头换方向 */

@keyframes arrowOpen {
  0% {
    transform: rotate(0);
  }
  100% {
    transform: rotate(180deg);
  }
}

/* 箭头换方向 */

@keyframes arrowClose {
  0% {
    transform: rotate(180deg);
  }
  100% {
    transform: rotate(0);
  }
}

@keyframes shrink {
  0% {
    transform: scale3d(0.8, 0.8, 0.8);
    -webkit-transform: scale3d(0.8, 0.8, 0.8);
    /* Safari and Chrome */
  }
  25% {
    transform: scale3d(1, 1, 1);
    -webkit-transform: scale3d(1, 1, 1);
    /* Safari and Chrome */
  }
  50% {
    transform: scale3d(0.8, 0.8, 0.8);
    -webkit-transform: scale3d(0.8, 0.8, 0.8);
    /* Safari and Chrome */
  }
  75% {
    transform: scale3d(1, 1, 1);
    -webkit-transform: scale3d(1, 1, 1);
    /* Safari and Chrome */
  }
  100% {
    transform: scale3d(1, 1, 1);
    -webkit-transform: scale3d(1, 1, 1);
    /* Safari and Chrome */
  }
}

@keyframes first {
  from {
    left: 100%;
  }
  to {
    left: -120%;
  }
}