@import '../../css-public/itemGoodsCard.acss';

.pageArea {
  overflow-x: hidden;
  overflow-y: auto;
  -webkit-overflow-scrolling: touch;
}

.signInArea {
  background: linear-gradient(-60deg, #ff4242, #ea223e);
}

.signInHead {
  position: relative;
}

.signInHeadBg {
  height: 675rpx;
  width: 100%;
}

.activityIn {
  position: absolute;
  top: 127rpx;
  right: 0;
  z-index: 20;
  width: 145rpx;
  height: 52rpx;
  border-radius: 20rpx;
  background-size: 100%;
}

.activityRules {
  border-radius: 20rpx 0 0 20rpx;
}

.am-modal-show {
  z-index: 50;
}

.am-modal {
  border-radius: 15rpx;
}

.rulerPop {
  width: 620rpx;
  height: 780rpx;
  box-sizing: border-box;
  padding: 0 40rpx 40rpx;
}

.rulerTit {
  color: #323232;
  font-size: 46rpx;
  text-align: center;
  padding-top: 35rpx;
  padding-bottom: 20rpx;
  font-weight: 600;
}

.rulerCon {
  width: 100%;
  height: 655rpx;
  overflow-x: hidden;
  overflow-y: auto;
  -webkit-overflow-scrolling: touch;
  text-align: left;
}

.rulerDetail {
  color: #666666;
  line-height: 1.6;
  font-size: 30rpx;
}

/******弹幕样式  start*******/

.barrage {
  position: absolute;
  z-index: 10;
  width: 100%;
  top: 100rpx;
  left: 0;
}

.barrageTip {
  position: absolute;
  white-space: nowrap;
  /* 防止向下换行*/
  animation-timing-function: linear;
  animation-fill-mode: none;
  z-index: 11;
  color: #ffffff;
  font-size: 24rpx;
  background: rgba(0, 0, 0, 0.3);
  box-sizing: border-box;
  padding: 4rpx 10rpx;
  border-radius: 20rpx;
}

.barrageTip.top {
  height: 60rpx;
}

.barrageTip.middle {
  height: 60rpx;
  top: 60rpx;
}

.barrageTip.bottom {
  height: 60rpx;
  top: 120rpx;
}

.barrageTxt {
  color: #ffffff;
  font-size: 24rpx;
  padding: 4rpx 10rpx;
  box-sizing: border-box;
  border-radius: 40rpx;
  background: rgba(0, 0, 0, .3);
  display: inline-flex;
  margin-right: 60rpx;
  white-space: nowrap;
}

@keyframes barrageAnimation {
  0% {
    transform: translateX(0);
  }
  100% {
    transform: translateX(-100%);
  }
}

/******弹幕样式  end*******/

.swipe {
  box-sizing: border-box;
  padding: 0 30rpx 30rpx;
}

.swiperItem {
  border-radius: 20rpx;
}

.max-height200 {
  max-height: 200rpx;
}

.calendar {
  box-sizing: border-box;
  padding: 0 30rpx 50rpx 30rpx;
}

.calendarTop {
  height: 102rpx;
  width: 100%;
  box-sizing: border-box;
  padding: 0 30rpx;
  background: url(https://img.sfddj.com/miniappImg/activity/signIn/calenderTop.png) no-repeat;
  background-size: 100%;
}

.totalAward {
  width: 100%;
  height: 84rpx;
  overflow-y: hidden;
  overflow-x: scroll;
  -webkit-overflow-scrolling: touch;
  text-align: center;
  position: relative;
  white-space: nowrap;
}

.hasNoAward {
  color: #ffb8a6;
  white-space: nowrap;
  font-size: 28rpx;
  line-height: 84rpx;
  /* overflow: hidden; */
}

.hasAward {
  position: relative;
  width: 100%;
  height: 100%;
  white-space: nowrap;
  /* overflow: hidden; */
}

.signAwardTip {
  font-size: 28rpx;
  line-height: 84rpx;
  /* white-space: nowrap; */
  position: absolute;
  white-space: nowrap;
  /* 防止向下换行*/
  animation-timing-function: linear;
  animation-fill-mode: none;
  z-index: 11;
  color: #ffffff;
  font-size: 24rpx;
  box-sizing: border-box;
}

.signAwardTip .signAwardNormal {
