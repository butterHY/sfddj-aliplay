.AdsImgShow .ads-white-interval {
    height: 0.384rem;
    width: 100%;
}

.couponModular {
  padding: 0 30rpx;
}

.couponTitle {
  height: 74rpx;
}

.couponConnent {
  /* height: 190rpx; */
}

.couponBox {
  width: 50%;
  height: 190rpx; 
  float: left;
  box-sizing: border-box;
  margin-bottom: 20rpx;
}

.coupon {
  height: 100%;
  background: skyblue;
  border-radius: 20rpx;
}

.couponMessage {
  background: #ffe6a1;
  border-radius: 20rpx 0 0 20rpx;
  padding-left: 20rpx;
}

.couponDescribe {
  margin-top: 18rpx;
}

.couponButton {
  height: 100%;
  width: 78rpx;
  background: #e4c672;
  border-radius: 0 20rpx 20rpx 0;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  
}

.dottedLine {
  height: 100%;
  border-style: none none none dotted;
  border-color: #fff;
  width: 2rpx;
  left: -2rpx;
}

.litlePoint {
  width: 4rpx;
  height: 4rpx;
  background: #f5f5f5;
}

.litlePointTop {
  top: -2rpx;
  left: -2rpx;
}

.litlePointBot {
  bottom: -2rpx;
  left: -2rpx;
}

.activity .couponBox {
  width: 100%;
  height: 160rpx;
}

.activity .couponButton {
  width: 188rpx;
  background: #e4c672;
  border-radius: 0 20rpx 20rpx 0;
}

.activity .couponButtonText {
    width: auto;
}

.activity .couponMessage {
  padding-left: 32rpx;
}


