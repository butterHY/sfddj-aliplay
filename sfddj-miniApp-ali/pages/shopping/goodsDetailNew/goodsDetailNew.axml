    </button>
  </view>



  <!-- 已选规格           已完成新接口字段更换-->
  <view class="division-area">
  </view>
  <view class="specifications-view selected-view" onTap="selectedSpecs">
    <view class="selectedSpecifications single-line-text flex_view_row" >
      <view class="font-26 color-a1a0a0">已选</view>
        <view a:if="{{specType == 'MULTI'}}" class="font-26 marLeft_30 single-line">{{multiformname}} , {{quantity}}件</view>
        <view a:else class="font-26 marLeft_30 single-line " >{{product.productName}} , {{quantity}}件</view>
    </view>
    <text class="icon-href-right font-22 color-c7c7cc padding-r15">
    </text>
  </view>

  <!-- 用户默认地址或者获取当前地址 已完成新接口字段更换-->
  <view class="line-view">
    <view class="one-line"></view>
  </view>
  <view class="store-view font-26" onTap="showAddressPop">
    <view class="selected-view">
      <view class="selectedSpecifications flex_view_row single-line-text">
        <view class="font-26 color-a1a0a0">送至</view>
        <view class="marLeft_30 single-line flex_view_row">
          <icon type="success" size="13" class="marRight_15"/>
          <block a:if="{{addressList && addressList.length > 0}}" a:for="{{addressList}}">
             <view class="single-line" a:if="{{item.isDefault}}">{{item.province}}{{item.city}}{{item.area}}{{item.address}}</view>
          </block>
          <block a:if="{{address}}">
            <view class="single-line">{{address}}</view>
          </block>
        </view>
      </view>
      <text class="icon-href-right font-22 color-c7c7cc padding-r15">
      </text>
    </view>
    <view class="addressTips">下单后<text class="color_red">24小时以内</text>发货</view>
  </view>
  <popup a:if="{{addressList && addressList.length > 0}}" show="{{isShowAddressPop}}" mask="{{true}}" position="bottom" animation="{{true}}" onClose="onAddressPop" zIndex="100" disableScroll="{{false}}">
    <view class="addressPop flex_view_col">
      <view class="flex_view_row justi_end" onTap="onAddressPop">
         <icon type="cancel" size="24" color="black"/>
      </view>
      <scroll-view scroll-y="{{true}}"  class="addressBox font-26 flex-1">
        <view class="addressMessage posi_re" a:for="{{addressList}}" data-addressid="{{item.id}}" onTap="selectAddress">
          <icon type="warn" size="12" color="{{item.isDefault ? 'red': 'black'}}" class="posi_ab marTop_5"/>
          <view class="{{item.isDefault ? 'color-ff5353': 'color-black'}} address"><text class="marRight_10">{{item.province}}</text><text class="marRight_10">{{item.city}}</text><text class="marRight_10">{{item.area}}</text><text class="marRight_10">{{item.address}}</text></view>
        </view>
      </scroll-view>
      <view class="addressButton flex_view_col justi_cen font-30">
        <navigator  url="/pages/user/addressManage/addressManage">添加其他地址</navigator>
      </view>
    </view>
  </popup>

  
  <!-- 如果是顺丰会员商品会显示奖励多少积分  -->
