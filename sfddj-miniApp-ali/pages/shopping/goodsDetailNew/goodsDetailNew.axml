        <view class="goodsTag label" a:if="{{goods.goodsTag}}">{{goods.goodsTag}}</view>
      </view>    
    </view>
    <button open-type="share" size="mini" plain="{{true}}" class="flex_view_col justi_between align_end shareButton posi_ab">
      <icon type="info" size="20"/>
