    <view class="division-area">
    </view>
  </view>


  <!--全赔说明   -->
  <view class="goodsCoverage" a:if="{{goods.afterSaleGueeList}}" onTap="showCompensationRuleFn">
    <view class="flex_view_row justi_between align_cen">
      <view class="flex_view_row">
        <text class="font-26 color-a1a0a0">保障                
        </text>
        <view class="marLeft_30">
