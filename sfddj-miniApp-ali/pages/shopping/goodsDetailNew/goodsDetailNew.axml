        </view>
        <view class="flex_1 flex_view_row justi_cen" data-current="1" onTap="switchSwiper">
          <text  class="{{titleCurrent == 1 ?  'active ' : 'headTitle'}}">平台券</text>
        </view>
      </view>
      <swiper indicator-dots="{{false}}" class="flex_1" onChange="intervalChange" current="{{swiperCurrent}}" duration="200">
        <block a:for="{{['#4A90E2','#F5A623']}}" >
          <swiper-item  >
              <view style="width:100%;height:100%" class="flex_view_col">
                  <scroll-view scroll-y="{{true}}" class="width_100 flex_1 whiteBack" >
                    <view class="coupon platCoupon">
                        <view class="couponBox">
                            <view class="couponInfo HBox">
                                <view class="couponInfoLeft HBox BoxCenter">
                                    <view class="couponPrice">
                                        <text>￥</text> <text class="moneyNumber">1</text>
                                        <view class="describe">满2元可用</view>
                                    </view>
                                </view>
                                <!--中间-->
                                <view class="couponInfoCenter Bflex1 height_100">
                                    <view class="couponUseType">
                                        <view class="singleLine">十大撒打算</view>
                                        <view class="tip_use"> </view>
                                        <view class="tip_useLimt Bflex1 termValidity">
                                            <view>2019.08.29-2020.08.29</view>
                                        </view>
                                    </view>
                                </view>
                                <view class="couponInfoRight HBox BoxCenter">
                                    <view class="useThisCoupon">
                                       <view class="useThisCouponBtn HBox BoxCenter js_getNow"
                                            data-couponid="116244"
                                            style="touch-action: pan-y; user-select: none; -webkit-user-drag: none; -webkit-tap-highlight-color: rgba(0, 0, 0, 0);">
                                            立即领取
                                        </view>
                                        <view class="useThisCouponBtn HBox BoxCenter" data-couponid="116244">已领取</view>
                                    </view>
                                </view>
                            </view>
                        </view>
                        <view class="gapBox gapBox_right">
                            <text class="gap gap_top"></text>
                            <text class="gap_line"></text>
                            <text class="gap gap_bot"></text>
                        </view>
                    </view>

                    <view class="coupon platCoupon">
                      <view class="couponBox">
                          <view class="couponInfo HBox">
                              <view class="couponInfoLeft HBox BoxCenter">
                                  <view class="couponPrice"> 
                                    <text>￥</text> <text class="moneyNumber">1</text>
                                    <view class="describe">满2元可用</view>
                                  </view>
                              </view>
                              <!--中间-->
                              <view class="couponInfoCenter Bflex1 height_100">
                                  <view class="couponUseType">
                                      <view class="singleLine">十大撒打算</view>
                                      <view class="tip_use"> </view>
                                      <view class="tip_useLimt Bflex1 termValidity">
                                          <view>2019.08.29-2020.08.29</view>
                                      </view>
                                  </view>
                              </view>
                              <view class="couponInfoRight HBox BoxCenter">
                                  <view class="useThisCoupon">
                                      <view class="useThisCouponBtn HBox BoxCenter js_getNow"
                                          data-couponid="116244"
                                          style="touch-action: pan-y; user-select: none; -webkit-user-drag: none; -webkit-tap-highlight-color: rgba(0, 0, 0, 0);">
                                          立即领取
                                      </view>
                                      <view class="useThisCouponBtn HBox BoxCenter" data-couponid="116244">已领取</view>
                                  </view>
                              </view>
                          </view>
                      </view>
                      <view class="gapBox gapBox_right">
                          <text class="gap gap_top"></text>
                          <text class="gap_line"></text>
                          <text class="gap gap_bot"></text>
                      </view>
                    </view>
                    
                    <view class="moreBtnBox js_moreBtnBox">
                      <view class="loadingBtn js_loadingBtn js_loadNull" style="">
                          <text class="information">没有更多</text>
                      </view>
                      <view class="loadingBtn js_loadingBtn js_loading" style="display: none;">
                          <text class="icon_loading iconStyle icon-spinner3 myRotateRefresh"></text>
                          <text class="information">加载中</text>
                      </view>
                    </view>
                  </scroll-view>
              </view>
          </swiper-item>
        </block>
      </swiper>
    </view>
  </popup>

<!-- 秒杀商品的价格        已完成新接口字段更换-->
  <view>
    <!-- goodsSecondKill 应该放置一个默认价格，因为这样在改动规格的时候倒计时模块展示的价格才不会跟着规格改变，原有版本并没有这个效果 -->
    <!-- a:if="{{goodsSecondKill}}" -->
    <!-- priceShow -->
    <spike-price a:if="{{secKillStatus}}" goodsSecondKill="{{goodsSecondKill}}" isFirstTime="{{isFirstTime}}" onSpikeOver="onSpikeOver" oldPrice="{{goods.defaultPrice}}"></spike-price> 
  </view>

    <!-- 商品价格、销量    已完成新接口字段更换-->
  <view a:if="{{!secKillStatus}}" class="detail-sale-view font-36" >
      <view class="color-ff5353"  a:if="{{!SFmember}}">
          <text>¥</text>
          <text decode="true" space="true">&nbsp;</text>
          <!-- priceShow -->
          <text class="font-56 font-weight-600">{{goods.defaultPrice}}</text>
          <text a:if="{{goods.jifenStatus}}"> + {{theMemberPoint}}积分            
          </text>
      </view>
      <view class="color-ff5353" a:if="{{SFmember}}">
          <!-- priceShow -->
          <text>¥{{goods.defaultPrice}}      
          </text>
          <text class="font-26 color-666"> + {{theCostMemberScore}}积分      
          </text>
      </view>
      <view class="color-a1a0a0 font-26">已售：{{goods.salesCount}}份
      </view>
  </view>
  
    <!-- 优惠券导航栏      这一部分的数据还没有更新 -->
  <view class="receiveCoupon flex_view_row justi_between font-26" onTap="showPopup">
    <view class="flex_view_row flex_1 justi_start">
      <view class="couponTitle">
        <image src="https://img.sfddj.com/miniappImg/public/goodsDetail/icon_coupon.png" class="width_100 height_100" mode="widthFix"></image>
      </view>
      <view class="couponName">满100减10</view>
      <view class="couponName">满20减5</view>
      <view class="couponName">满1000减100</view>
    </view>
    <view>
      <text class="color-a1a0a0">领券</text>
      <text class="icon-href-right font-22 color-c7c7cc marLeft_20"></text>
    </view>
  </view>

  <!-- 商品名称、title、标签  已完成新接口字段更换-->
  <view class="detail-header-view posi_re ">
    <view class="detail-header-name color-323232 font-36">
      <view class="twoLineHide">
        <text class="labelName font-22 " a:if="{{goods.goodsLabels}}">{{goods.goodsLabels}}</text>
        <text class="font-weight-600" >{{goods.goodsName}}</text>   
      </view> 
      <view class="flex_view_row  justi_start font-20 labelList" a:if="{{that.data.goods.goodsViceVO.labelList.length > 0 || goods.goodsTag}}">
