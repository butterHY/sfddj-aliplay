.comeBack {
  position: fixed;
  z-index: 1000;
  bottom: 200rpx;
  right: -93rpx;
  background: rgba(0, 0, 0, .7);
  border-radius: 15rpx 0 0 15rpx;
}

.goIcon {
  height: 92rpx;
  width: 92rpx;
  text-align: center;
  color: #FFFFFF;
  line-height: 92rpx;
  border-bottom: 1rpx solid #3D3D3D;
  position: relative;
  border-radius: 15rpx 0 0 15rpx;
}

.comeBackDisplay {
  animation: comeBackMoveShow 0.3s linear;
  right: 0 !important;
}

@keyframes comeBackMoveShow {
  0% {right: -93rpx;}
  50% {right: -45;}
  100% {right: 0;}
}

.comeBackHide {
  animation: comeBackMoveHide 0.3s linear;
  right: -93rpx !important;
}

@keyframes comeBackMoveHide {
  0% {right: 0;}
  50% {right: -45;}
  100% {right:-93rpx;}
}

.navigation {
  z-index: -1;
  /* background: red; */
  opacity: 0;
  transition-property: z-index , opacity;
  transition-duration: 1s , 1s;
  -webkit-transition-property: z-index , opacity;
  -webkit-transition-duration: 1s , 1s;
	/* transition: opacity 2s; */
	/* -webkit-transition: opacity 2s; */
   /* Safari */
}

.opacityOne {
  opacity: 1;
  z-index: 99999999;
}
