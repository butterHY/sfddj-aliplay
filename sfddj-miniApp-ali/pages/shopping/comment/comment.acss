page {
	background: #f3f3f3;
}

.goods-comment-title {
	display: flex;
	flex-direction: row;
	justify-content: space-between;
	align-items: center;
	border-bottom: 2rpx #e6e6e6 solid;
	padding: 30rpx;
	font-size: 26rpx;
	color: #323232;
}

.goods-comment-area {
	display: flex;
	flex-direction: column;
	padding: 30rpx;
}

.comment-user-area {
	display: flex;
	flex-direction: row;
	justify-content: space-between;
	align-items: flex-end;
}

.comment-user-image {
	width: 66rpx;
	height: 66rpx;
	border-radius: 50%;
	overflow: hidden;
}

.comment-text {
	text-overflow: -o-ellipsis-lastline;
	overflow: hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-line-clamp: 2;
	line-clamp: 2;
	-webkit-box-orient: vertical;
}


/* 新版  ---190826 */

.all_100vh {
	width: 100%;
	height: 100vh;
