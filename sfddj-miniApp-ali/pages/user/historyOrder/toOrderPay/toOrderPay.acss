page {
  background: white;
}
.address-view {
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 28rpx 0 34rpx 0;
  width: 100%;
}
.address-info-view {
  display: flex;
  flex-direction: column;
  justify-content: center;
  height: 100%;
}
.address-phone-number {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.should-pay-area {
  display: flex;
  flex-direction: column;
  padding: 0 30rpx 0 30rpx;
}
.should-pay-text {
  display: flex;
  flex-direction: row;
  justify-content: center;
  padding: 20rpx 0 20rpx 0;
  align-items: center;
  border-bottom: 2rpx #eaeaea solid;
}
.should-pay-item {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 15rpx 0 15rpx 0;
  align-items: center;
  color: #323232;
  font-size: 24rpx;
}
.choice-coupon-view {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 30rpx 0 30rpx 0;
  align-items: center;
  border-bottom: 2rpx #eaeaea solid;
  border-top: 2rpx #eaeaea solid;
}
.coupon-name-view {
  display: flex;
  flex-direction: row;
  align-items: center;
}
.goods-info-area {
  display: flex;
  flex-direction: column;
  padding: 0 30rpx 0 30rpx;
}
.goods-item-title-view {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  padding: 20rpx 0 20rpx 0;
  border-bottom: 2rpx #eaeaea solid;
}
.goods-item-title-part {
  display: flex;
  flex-direction: row;
  align-items: center;
}
.goods-info-view {
  display: flex;
  flex-direction: row;
  padding: 20rpx 0 20rpx 0;
  border-bottom: 2rpx #eaeaea solid;
}
.goods-info-text-view {
  display: flex;
  flex-direction: column;
  margin-left: 20rpx;
  width: 100%;
}
.price-number-view {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.bottom-view {
  width: 100%;
  height: 100rpx;
  background: white;
  position: fixed;
  bottom: 0;
  display: flex;
  flex-direction: row;
  align-items: center;
  border-top: #e6e6e6 2rpx solid;
  z-index: 9000;
}
.bottom-text-red-bg {
  display: flex;
  width: 35%;
  height: 100%;
  background: #ff5353;
  color: white;
  justify-content: center;
  align-items: center;
  margin-left: 30rpx;
}
.invoice-view {
  display: flex;
  flex-direction: row;
  box-sizing: border-box;
  height: 88rpx;
  padding: 0 30rpx 0 30rpx;
  justify-content: space-between;
  align-items: center;
}
.to-gift-view {
  display: flex;
  flex-direction: row;
  box-sizing: border-box;
  padding: 30rpx;
  justify-content: space-between;
  align-items: center;
}
.name-input-view {
  border-top: #e6e6e6 2rpx solid;
  height: 88rpx;
  font-size: 26rpx;
}
/*头部地址   */
