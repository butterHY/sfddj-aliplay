}
/*积分商品的积分消耗   */
.jiFenUseBtn {
  width: 102rpx;
  height: 62rpx;
  border-radius: 45px;
}
.jiFenGreenBtn {
  background-color: #4dd865;
}
.jiFenGrayBtn {
  background-color: #c4c7c4;
}
.jiFenUseBtn_cir {
  width: 56rpx;
  height: 56rpx;
  border-radius: 50%;
  transition: 0.2s ease-in-out;
}
.jiFenGreenBtn_cir {
  background-color: #fff;
  position: absolute;
  right: 3rpx;
  top: 3rpx;
}
.jiFenGrayBtn_cir {
  background-color: #dadada;
  position: absolute;
  left: 3rpx;
  top: 3rpx;
}