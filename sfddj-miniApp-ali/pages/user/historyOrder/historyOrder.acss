  bottom: 0;
  left: 0;
  right: 0;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  z-index: 110;
}

.showDialogCon {
  background-color: white;
  border-radius: 10rpx;
  width: 500rpx;
  text-align: center;
}

.showDialog_btn {
  margin-top: 48rpx;
  display: flex;
  height: 70rpx;
}

.showDialog_btnCan {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
  border-top: solid #e6e6e6 2rpx;
}

.showDialog_btnSure {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #fb2c36;
  border-bottom-right-radius: 10rpx;
}

