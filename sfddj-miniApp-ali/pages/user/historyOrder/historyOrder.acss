}

.order-top {
  width: 100%;
  background-color: white;
  height: 80rpx;
  color: #838383;
  display: flex;
  justify-content: space-around;
}

.order-top-tab {
  flex: 1;
  text-align: center;
  line-height: 80rpx;
  position: relative;
}

.order-top-tab-num {
