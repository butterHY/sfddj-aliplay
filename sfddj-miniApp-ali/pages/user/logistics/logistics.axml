          <view a:if="{{fatherItem.isCheckMore}}" >收起物流详情 <text class="icon-href-up checkMoreIcon"></text></view>
          <view a:else="{{!fatherItem.isCheckMore}}" >点击查看更多物流详情<text class="icon-href-down checkMoreIcon"></text></view>
        </view>
      </view>
